{
  "expo": {
    "newArchEnabled": true,
    "name": "capstone-FE",
    "slug": "capstone-FE",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/main/assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/main/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/main/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/main/assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./src/main/assets/fonts/NotoSansKR-Regular.ttf",
            "./src/main/assets/fonts/NotoSansKR-Medium.ttf",
            "./src/main/assets/fonts/NotoSansKR-Bold.ttf"
          ]
        }
      ]
    ]
  }
}
